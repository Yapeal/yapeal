{
    "name": "yapeal/yapeal",
    "description": "Yet Another Php Eve Api Library also know as Yapeal which can be used to access the Eve Online API data and place it into a database.",
    "keywords": ["curl", "database", "Eve", "Eve-Online", "mysql", "mysqli", "ADOdb", "REST", "RESTful"],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Michael Cummings",
            "email": "dragonaire@users.sourceforge.net",
            "role": "Developer"
        },
        {
            "name": "Somerset Mahm",
            "email": "somersetmahm@users.sourceforge.net",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/Dragonrun1/yapeal",
    "support": {
        "forum": "https://forums.eveonline.com/default.aspx?g=posts&t=7540",
        "issues": "https://github.com/Dragonrun1/yapeal/issues?state=open",
        "source": "https://github.com/Dragonrun1/yapeal",
        "wiki": "https://github.com/Dragonrun1/yapeal/wiki"
    },
    "require": {
        "ext-curl": "*",
        "ext-date": "*",
        "ext-hash": "*",
        "ext-mysql": "*",
        "ext-mysqli": "*",
        "ext-openssl": "*",
        "ext-Reflection": "*",
        "ext-SimpleXML": "*",
        "ext-SPL": "*",
        "ext-xmlreader": "*",
        "lib-curl": ">=7.15.0",
        "lib-openssl": ">=0.9.8",
        "php": ">=5.3.2"
    },
    "require-dev": {
        "ext-xsl": "*",
        "phpdocumentor/phpdocumentor": "dev-develop"
    }
}
